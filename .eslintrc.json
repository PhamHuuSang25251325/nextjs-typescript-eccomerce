{
    "root": true,
    "parserOptions": {
        "project": "./tsconfig.json"
    },
    "extends": [
        "airbnb-typescript",
        "plugin:react-hooks/recommended", 
        "prettier/@typescript-eslint",
        "plugin:prettier/recommended"
    ],
    "plugins": ["prettier","react","react-hooks"],
    "rules": {
        "no-console": 0
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "commonjs": true
      }
}