{
    "root": true,
    "parserOptions": {
        "project": "./tsconfig.json"
    },
    "extends": [
        "airbnb-typescript",
        "plugin:react-hooks/recommended", 
        "prettier/@typescript-eslint",
        "plugin:prettier/recommended"
    ],
    "plugins": ["prettier","react","react-hooks"],
    "rules": {
        "no-console": 0,
        "import/prefer-default-export": 0,
        "react/jsx-props-no-spreading": 0,
        "react/prop-types": 0,
        "prettier/prettier": [
            "error",
            {
              "endOfLine": "auto"
            }
          ]
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "commonjs": true
      }
}